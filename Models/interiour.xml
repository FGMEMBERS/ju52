<!--cabin equipment 
Variant		Description

	0				civil, no Freightdoor,11 seats
	1				reserved for oher seat Variants
	2				reserved for oher seat Variants
	3				reserved for oher seat Variants
	4				reserved for oher seat Variants

	5				military, Freightdoor
	6				military, Freightdoor, Cargo Equipment 
	7				military, Freightdoor, 11 Seats
	8				military, Freightdoor, folding Seats

-->

 <animation>
  <type>select</type>
  <object-name>rear.windows.I</object-name>
  <object-name>rear.wglass.I</object-name>
  <object-name>interiour.civ.floor</object-name>
  <object-name>interiour.civ</object-name>
  <object-name>curtains</object-name>
  <condition>
   <less-than>
    <property>sim/model/variant</property>
    <value>5</value>
   </less-than>
  </condition>
 </animation>
 <animation>
  <type>select</type>
  <object-name>rear.door1.I</object-name>
  <object-name>rear.door2.I</object-name>
  <object-name>interiour.mil.floor</object-name>
  <object-name>interiour.mil</object-name>
  <object-name>interiour.struts</object-name>
	<object-name>roofhatch.I</object-name>
  <condition>
   <greater-than>
    <property>sim/model/variant</property>
    <value>4</value>
   </greater-than>
  </condition>
 </animation>
 <animation>
  <type>select</type>
  <object-name>seat1</object-name>
  <object-name>seat2</object-name>
  <object-name>seat3</object-name>
  <object-name>seat4</object-name>
  <condition>
		<or>
   		<equals>
    			<property>sim/model/variant</property>
    			<value>0</value>
   		</equals>
   		<equals>
    			<property>sim/model/variant</property>
    			<value>6</value>
   		</equals>
		</or>
  </condition>
 </animation>

